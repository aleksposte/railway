<%= form_for [:admin, @railway_station] do |f| %>
  
  <!-- Сообщения об ошибках вынесены в 'common/errors' -->
  <%= render 'common/errors', resource: @railway_station %>

  <div class="field">
    <%= f.label :title, "Название станции:" %><br>
    <%= f.text_field :title %>
  </div>

  <div class="actions">
    <%= f.submit %>
  </div>
<% end %>


